        @await RenderSectionAsync("Vendors", required: false)

        <!-- App scripts -->
        <script src="~/vendors/select2/js/select2.min.js"></script>
        <script src="~/vendors/datepicker/daterangepicker.js"></script>
        <script src="~/vendors/input-mask/jquery.mask.js"></script>
        <script src="~/js/app.js"></script>
        <script src="~/js/site.js"></script>
        <script src="~/js/jsTree3/jstree.min.js"></script>

        <script>
            "use strict";
            function CloseModal() {
                $('#form-modal').modal('hide');
            }
            $(window).on('load', function () {

                if ($('main.main-content').length > 0) {

                    $(document).on('click', '.navigation .navigation-menu-body a', function (e) {
                        e.preventDefault();

                        var url = ($(this).attr('href'));

                        if (url.indexOf('#') == -1) {
                            $('main.main-content').load(url + '/?IsRendred=true');

                            if ($('.navigation .navigation-menu-body a').hasClass('active'))
                                $('.navigation .navigation-menu-body a').removeClass('active');

                            if (!$(this).hasClass('active'))
                                $(this).addClass('active');

                            window.history.pushState($(this).text(), "Title", $(this).attr('href'));
                            $('head title').html($(this).text() + ' - Demo.Web');
                        }
                    });
                }

                if ($('.change-theme').length > 0) {
                    changeTheme('.change-theme');

                    $(document).on('change', '.change-theme', function (e) {
                        e.preventDefault();
                        changeTheme(this);
                    });
                }

                getUserProfile();


            });
            function changeTheme(element) {

                if ($(element).is(':checked')) {
                    $(element).parents().eq(0).find('i').attr('class', 'fa fa-sun-o');

                    if (!$('body').hasClass('dark'))
                        $('body').addClass('dark');
                    setUserProfile(3, false);
                }
                else {
                    $(element).parents().eq(0).find('i').attr('class', 'fa fa-moon-o');

                    if ($('body').hasClass('dark'))
                        $('body').removeClass('dark');
                    setUserProfile(3, true);
                }
            }
            function changeSidebar(sideClass, kind, fromGet) {
                if (sideClass == 'default') {
                    $('body').removeClass();

                } else {

                    if (!$('body').hasClass(sideClass))
                        $('body').addClass(sideClass);
                }
                if (fromGet == undefined)
                    setUserProfile(2, kind);
            }
            function toggleFullScreen() {
                if (!document.fullscreenElement &&    // alternative standard method
                    !document.mozFullScreenElement && !document.webkitFullscreenElement && !document.msFullscreenElement) {  // current working methods
                    if (document.documentElement.requestFullscreen) {
                        document.documentElement.requestFullscreen();
                    } else if (document.documentElement.msRequestFullscreen) {
                        document.documentElement.msRequestFullscreen();
                    } else if (document.documentElement.mozRequestFullScreen) {
                        document.documentElement.mozRequestFullScreen();
                    } else if (document.documentElement.webkitRequestFullscreen) {
                        document.documentElement.webkitRequestFullscreen(Element.ALLOW_KEYBOARD_INPUT);
                    }
                    setUserProfile(1, true);
                } else {

                    if (document.exitFullscreen) {
                        document.exitFullscreen();
                    } else if (document.msExitFullscreen) {
                        document.msExitFullscreen();
                    } else if (document.mozCancelFullScreen) {
                        document.mozCancelFullScreen();
                    } else if (document.webkitExitFullscreen) {
                        document.webkitExitFullscreen();
                    }
                    setUserProfile(1, false);
                }
            }
            function setUserProfile(kind, val) {

                $.ajax({
                    url: '/Auth/SetUserProfile',
                    type: "POST",
                    dataType: "JSON",
                    data: { insertKind: kind, value: val },
                    success: function (res) {


                    }
                });
            }


            function getUserProfile() {

                $.ajax({
                    url: '/Auth/GetUserProfile',
                    type: "POST",
                    dataType: "JSON",
                    success: function (res) {

                        switch (res.usersProfile_TemplateKind) {
                            case 1:
                                changeSidebar('default', 1, true);
                                break;
                            case 2:
                                changeSidebar('small-navigation', 2, true);
                                break;
                            case 3:
                                changeSidebar('hidden-navigation', 3, true);
                                break;
                        }
                        if (res.usersProfile_isLight == false) {
                            if (!$('body').hasClass('dark'))
                                $('body').addClass('dark');
                        } else {
                            if ($('body').hasClass('dark'))
                                $('body').removeClass('dark');
                        }
                        if (res.usersProfile_isFullscreen == true) {
                            if (!document.fullscreenElement &&    // alternative standard method
                                !document.mozFullScreenElement && !document.webkitFullscreenElement && !document.msFullscreenElement) {  // current working methods
                                if (document.documentElement.requestFullscreen) {
                                    document.documentElement.requestFullscreen();
                                } else if (document.documentElement.msRequestFullscreen) {
                                    document.documentElement.msRequestFullscreen();
                                } else if (document.documentElement.mozRequestFullScreen) {
                                    document.documentElement.mozRequestFullScreen();
                                } else if (document.documentElement.webkitRequestFullscreen) {
                                    document.documentElement.webkitRequestFullscreen(Element.ALLOW_KEYBOARD_INPUT);
                                }

                            }
                        }
                    }
                });
                var urlGetCurrDate = '@Url.Action("GetCurrentDate", "Home")';
                $.ajax({
                    url: urlGetCurrDate,//'~/Home/GetCurrentDate',
                    type: "GET",
                    dataType: "JSON",

                    success: function (res) {

                        $('#lnkCurrentDate').html(res);
                        $('#btnWorkday').val('Workday : ' + res);

                    }
                });
            }
            function showCurrentDate() {
                var urlSetCurrDate = '@Url.Action("SetCurrentDate", "Home")';
                showInPopup(urlSetCurrDate/*"~/Home/SetCurrentDate"*/, "Current Date");
            }
            $(document).on({
                ajaxStart: function () {
                    $("body").addClass("loading");
                },
                ajaxStop: function () {
                    $("body").removeClass("loading");
                }
            });
        </script>

        @await RenderSectionAsync("Scripts", required: false)
    </body>
